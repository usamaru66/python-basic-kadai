class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age
  def check_adult(self):
    if self.age>=20:
      print("大人である。")
    else:
      print("大人ではない。")

tarou=Human("太郎",22)
zirou=Human("次郎",20)
sannrou=Human("三郎",18)

human=[tarou,zirou,sannrou]
for person in human:
  person.check_adult()
