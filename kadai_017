class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age
  def check_adult(self):
    if self.age>=20:
      print(f"{name}は大人である。")
    else:
      print(f"{name}は大人ではない。")

user_date={"太郎":22,"次郎":20,"三郎":18}
for name,age in user_date.items():
  person=Human(name,age)
  person.check_adult()
